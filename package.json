{
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "7.1.0",
        "@fortawesome/free-brands-svg-icons": "7.1.0",
        "@fortawesome/free-regular-svg-icons": "7.1.0",
        "@fortawesome/free-solid-svg-icons": "7.1.0",
        "@fortawesome/react-fontawesome": "3.1.1",
        "date-fns": "4.1.0",
        "react": "19.2.3",
        "react-day-picker": "9.13.0",
        "react-dom": "19.2.3",
        "react-hot-toast": "2.6.0",
        "zustand": "5.0.9"
    },
    "devDependencies": {
        "@eslint/js": "9.39.2",
        "@rsbuild/core": "1.6.15",
        "@rsbuild/plugin-react": "1.4.2",
        "@tailwindcss/postcss": "4.1.18",
        "@types/node": "25.0.3",
        "@types/react": "19.2.7",
        "@types/react-dom": "19.2.3",
        "concurrently": "9.2.1",
        "eslint": "9.39.2",
        "eslint-plugin-react-hooks": "7.0.1",
        "eslint-plugin-react-refresh": "0.4.26",
        "globals": "16.5.0",
        "prettier": "3.7.4",
        "prettier-plugin-organize-imports": "4.3.0",
        "prettier-plugin-tailwindcss": "0.7.2",
        "tailwindcss": "4.1.18",
        "typescript": "5.9.3",
        "typescript-eslint": "8.50.1"
    },
    "name": "system-monitor-key",
    "private": true,
    "scripts": {
        "build": "pnpm build:api && pnpm build:frontend",
        "build:api": "cd public/api && composer update --no-interaction && composer install --no-dev --optimize-autoloader",
        "build:frontend": "eslint . && rsbuild build",
        "dev": "concurrently \"pnpm dev:api\" \"pnpm dev:frontend\"",
        "dev:api": "cd public/api && php -S localhost:8000 -t .",
        "dev:frontend": "rsbuild dev",
        "format": "prettier --write .",
        "install:api": "cd public/api && composer install",
        "lint": "eslint .",
        "preview": "rsbuild preview"
    },
    "type": "module",
    "version": "1.0.0"
}
